SELECT b.is_open, SUM(c.count) AS checkin_total 
FROM business b 
LEFT JOIN checkin c
ON b.id = c.business_id
GROUP BY is_open

+---------+---------------+
| is_open | checkin_total |
+---------+---------------+
|       0 |            15 |
|       1 |           823 |
+---------+---------------+


