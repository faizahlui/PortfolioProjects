SELECT c.business_id,b.name, COUNT(*)
FROM business b  
LEFT JOIN category c
ON c.business_id = b.id 
LEFT JOIN review r 
ON c.business_id = r.business_id
WHERE c.category = 'Restaurants' AND b.city ='Las Vegas'
GROUP BY b.id 


